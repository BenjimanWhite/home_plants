<header>
  <div class="container">
    <nav>
      <ul>
        <li><%= link_to "Home", root_path %></li>
        <% if logged_in? %>
          <li>
            <ul>
              <%= link_to "Settings", edit_user_path(current_user) %>
              <li><%= link_to "Log out", logout_path, method: :delete %></li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
          <li><%= link_to "Sign up now!", signup_path %></li>

        <% end %>
      </ul>
    </nav>
  </div>
</header>